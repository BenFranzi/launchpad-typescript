    "@types/node": "^14.11.2",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "module-alias": "^2.2.3",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.10.2",
    "typescript": "~4.7.0"
  }
