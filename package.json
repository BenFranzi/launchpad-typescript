  "scripts": {
    "start": "npm run dev",
    "dev": "node --loader ts-node/esm -r tsconfig-paths/register src/index.ts",
    "dev2": "ts-node -r tsconfig-paths/register ./src/index.ts",
    "dev3": "node --loader ts-node/esm -r tsconfig-paths/register src/index.ts --import 'data:text/javascript,import { register } from \"node:module\"; import { pathToFileURL } from \"node:url\"; register(\"ts-node/esm\", pathToFileURL(\"./\"));'",
    "dev4": "ts-node src/index.ts",
    "dev5": "ts-node --compiler-options '{\\\"module\\\": \\\"commonjs\\\"}' src/index.ts",
    "dev6": "tsx src/index.ts",
    "test": "jest"
  },
  "devDependencies": {
    "@bengineer.dev/eslint-config-core": "^1.0.4",
